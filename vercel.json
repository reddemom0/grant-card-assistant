{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/grant-cards",
      "dest": "/grant-cards.html"
    },
    {
      "src": "/etg-writer", 
      "dest": "/etg-agent.html"
    },
    {
      "src": "/bcafe-writer",
      "dest": "/bcafe-agent.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
