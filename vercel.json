{
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth-google",
      "dest": "/api/auth-google.js"
    },
    {
      "src": "/api/auth-callback",
      "dest": "/api/auth-callback.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/grant-cards",
      "dest": "/grant-cards.html"
    },
    {
      "src": "/etg-writer", 
      "dest": "/etg-agent.html"
    },
    {
      "src": "/bcafe-writer",
      "dest": "/bcafe-agent.html"
    },
    {
      "src": "/canexport-claims",
      "dest": "/canexport-claims.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
